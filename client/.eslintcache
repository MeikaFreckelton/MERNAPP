onsole.log(nextId)\n        // console.log(newPost._id)\n        // console.log(match.params)\n        // dispatch(getAllPosts())\n    }\n    console.log(formState)\n\n\n    return(\n        <div>\n            \n            \n            <div id=\"addPostDiv\">\n                <div className=\"heading\">\n                    <h1>Add post</h1>\n                </div>\n                <form id=\"addPostForm\" onSubmit={handleSubmit}>\n\n                    <label>Post Title</label>\n                    <FormInput name=\"title\" placeholder=\"Title here...\" className=\"addPostField\" onChange={handleChange} />\n\n                    <label>Post Category</label>\n                    <FormInput name=\"category\" placeholder=\"Category here...\" className=\"addPostField\" onChange={handleChange} />\n\n                    <label>Post Content</label>\n                    <textarea\n                        name=\"text\"\n                        placeholder=\"content here...\"\n                        className=\"addPostField\"\n                        onChange={handleChange}\n                        \n                    />\n                    <input \n                    type=\"submit\"\n                    className=\"addPostSubmit\"\n                    value=\"New Post\"\n                    />\n\n                </form>\n            </div>\n        </div>\n    )\n}\n\n\nexport default AddPost","/Users/meika/Desktop/mern-app/client/src/components/posts/EditPost.js",[],"/Users/meika/Desktop/mern-app/client/src/components/profiles/Profile.js",["220","221","222","223","224","225","226","227"],"import React, { useState, useEffect } from 'react'\nimport { Link } from 'react-router-dom'\n\n// how to map over links and display them with matching icon?\n\nconst Profile = ({ profile }) => {\n    console.log('DETAILS', profile)\n\n    // destructuring data variables\n    const { details, skills, currentroles, pastroles, prospects, qualifications, socials, portfolio } = profile\